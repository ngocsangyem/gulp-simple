	},
	"homepage": "https://github.com/ngocsangyem/gulp-simple#readme",
	"componentOptions": {
		"test": true,
		"htmlOptions": "pug",
		"cssOptions": "sass",
		"jsOptions": "javascript",
		"extensions": {
			"html": "pug",
			"script": "js",
			"test": "test.js",
			"css": "sass"
		}
	}
